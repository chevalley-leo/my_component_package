{
    "model": {
        "file_path": "piece.stl",
        "scale": 0.001,
        "sample_points": 5000,
        "z_min_tolerance": 0.0001
    },
    "camera": {
        "depth_stream": {
            "width": 1280,
            "height": 720,
            "format": "z16",
            "fps": 30
        },
        "color_stream": {
            "width": 1280,
            "height": 720,
            "format": "bgr8",
            "fps": 30
        },
        "depth_trunc": 3.0,
        "depth_scale": 1000.0
    },
    "filter": {
        "target_color": [128, 96, 49],
        "tolerance_positive": [20, 20, 20],
        "tolerance_negative": [30, 30, 30]
    },
    "dbscan": {
        "eps": 0.01,
        "min_samples": 10
    }
}